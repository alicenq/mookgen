{
    "compileOnSave": true,
    "compilerOptions": {
        "allowUnreachableCode": false,
        "declaration": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noLib": false,
        "outDir": "src",
        "rootDir": "types",
        "removeComments": true,
        "target": "es5"
    }
}